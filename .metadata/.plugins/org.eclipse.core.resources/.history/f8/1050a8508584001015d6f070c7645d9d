package com.yedam.collect;

public class EmpVO {
	//필드 선언
	private int enpno;
	private String ename;
	private int dept;
	
	//empno, ename 생성자
	public EmpVO(int enpno, String ename) {
		this.enpno = enpno;
		this.ename = ename;
	}

	public int getEnpno() {
		return enpno;
	}

	public void setEnpno(int enpno) {
		this.enpno = enpno;
	}

	public String getEname() {
		return ename;
	}

	public void setEname(String ename) {
		this.ename = ename;
	}
	
	public int getDept() {
		return dept;
	}

	public void setDept(int dept) {
		this.dept = dept;
	}
}
